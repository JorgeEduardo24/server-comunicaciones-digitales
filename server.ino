//Configuración Red Inalambrica
const char* ssid = "scd";
const char* password = "12345678";

// Configuración del servidor como Access Point
const IPAddress apIP(192, 168, 4, 1);
const IPAddress netMsk(255, 255, 255, 0);

//Scketch Files
String videoBitFilePath = "/video_binary.txt";
String videoFilePath = "/video.mp4";

//Data Source
