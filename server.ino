// Configuración del servidor web 
AsyncWebServer server(80);
    
void setup() {
  Serial.begin(115200);
  
  // Iniciar el modo Access Point
  WiFi.mode(WIFI_AP);
  WiFi.softAPConfig(apIP, apIP, netMsk);
  WiFi.softAP(ssid, password);
  Serial.println("Access Point iniciado");

  // Inicializar SPIFFS
  if (!SPIFFS.begin()) {
    Serial.println("Error al inicializar SPIFFS");
    return;
  }
   
  // Configurar el enrutamiento del servidor web
  server.on("/bit", HTTP_GET, [](AsyncWebServerRequest *request){
    Serial.println("Endpoint bits download Request"); 
    if (request->hasParam("param")) {
      // Obtener el valor del parámetro entero enviado desde el cliente
      int paramValue = request->getParam("param")->value().toInt();
      Serial.println(paramValue);
      AsyncWebServerResponse *response = request->beginResponse(200, "text/plain", dataBit[paramValue]);
      response->addHeader("Content-Disposition","attachment; filename=\"file.txt\"");
      request->send(response);
    } else {
      // Si no se proporciona el parámetro, enviar un código de error
      request->send(400, "text/plain", "Parámetro requerido");
    }
  });

  server.on("/test", HTTP_GET, [](AsyncWebServerRequest *request){
    Serial.println("Endpoint test download Request"); 
    AsyncWebServerResponse *response = request->beginResponse(200, "text/plain", "Hola");
    response->addHeader("Content-Disposition","attachment; filename=\"text.txt\"");
    request->send(response);
  });

   server.on("/videoBit", HTTP_GET, [](AsyncWebServerRequest *request){
    // Leer el archivo de video en bloques y transmitirlos al cliente
    File videoFile = SPIFFS.open(videoBitFilePath, "r");
    if (!videoFile) {
      Serial.println("Error al abrir el archivo de video");
      request->send(404);
      return;
    }
    while(videoFile.available()){
      Serial.write(videoFile.read());  
    }
    Serial.println("Endpoint video text file Request");
    // Establecer las cabeceras de respuesta para la descarga
    AsyncWebServerResponse *response = request->beginResponse(videoFile,videoBitFilePath, "text/plain", true);
    // Enviar la respuesta al cliente
    request->send(response);
    videoFile.close();
  });

   server.on("/video", HTTP_GET, [](AsyncWebServerRequest *request){
    // Leer el archivo de video en bloques y transmitirlos al cliente
    File videoFile = SPIFFS.open(videoFilePath, "r");
    if (!videoFile) {
      Serial.println("Error al abrir el archivo de video");
      request->send(404);
      return;
    }
    Serial.println("Endpoint video file Request");
    // Establecer las cabeceras de respuesta para la descarga
    AsyncWebServerResponse *response = request->beginResponse(videoFile,videoFilePath, "video/mp4", true);
    // Enviar la respuesta al cliente
    request->send(response);
    videoFile.close();
  });
  
  server.onNotFound([](AsyncWebServerRequest *request){
    Serial.println("Endpoint not found Request"); 
    request->send(400,"text/plain","Not Found");
  });

  // Iniciar el servidor web
  server.begin();
  Serial.println("Servidor web iniciado");
}



void loop() {
  // Principal Code
}
